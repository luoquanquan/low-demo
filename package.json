{
  "name": "parcel-react-client-starter",
  "private": true,
  "version": "0.0.0",
  "source": "src/index.html",
  "targets": {
    "default": {
      "source": "./src/index.html",
      "distDir": "./docs",
      "publicUrl": "/low-demo/"
    },
    "vercel": {
      "source": "./src/vercel.html",
      "distDir": "dist",
      "publicUrl": "/"
    }
  },
  "scripts": {
    "start": "rimraf docs && parcel",
    "build": "rimraf docs && parcel build",
    "build-vercel": "rimraf dist && parcel build --target=vercel && mv ./dist/vercel.html ./dist/index.html"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "parcel": "^2.14.0",
    "rimraf": "^6.0.1"
  }
}
